<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="70dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingStart="10dp"
        android:paddingEnd="10dp">

        <TextView
            android:id="@+id/row_contraction_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="12/16/2016"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/row_contraction_time"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/row_contraction_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="16:18:23"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/row_contraction_last"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="18:23"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/row_contraction_duration"
            app:layout_constraintStart_toEndOf="@+id/row_contraction_time" />

        <TextView
            android:id="@+id/row_contraction_duration"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:text="05:11"
            android:textSize="25sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>